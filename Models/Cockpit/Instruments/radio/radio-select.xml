<!--  SELECT - KNOB -->
   <animation>
        <type>rotate</type>
        <object-name>selKnob</object-name>
        <property>/instrumentation/radio-select/switch</property>
        <interpolation>
          <entry><ind>0</ind><dep>45</dep></entry>
          <entry><ind>1</ind><dep>0</dep></entry>
          <entry><ind>2</ind><dep>-45</dep></entry>
          <entry><ind>3</ind><dep>-180</dep></entry>
        </interpolation>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m> 0.0</x-m>
            <y-m> 0.022</y-m>
            <z-m> 0.01</z-m>
        </center>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>selPlane</object-name>
        <visible>false</visible>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
              <command>property-adjust</command>
              <property>/instrumentation/radio-select/switch</property>
              <step>1</step>
              <min>0</min>
              <max>4</max>
              <wrap>1</wrap>
            </binding>
            <binding>
              <command>property-toggle</command>
              <property>/sim/sound/switch2</property>
            </binding>
        </action>

        <action>
            <button>1</button>
            <interval-sec>0.2</interval-sec>
            <binding>
