      <equals>
        <property>/instrumentation/radio-select/switch</property>
        <value>3</value>
      </equals>
    </condition>
</animation>

<animation>
    <type>select</type>
    <object-name>tacanX</object-name>
    <condition>
      <not-equals>
        <property>/instrumentation/radio-select/switch</property>
        <value>0</value>
      </not-equals>
      <not-equals>
        <property>/instrumentation/radio-select/switch</property>
        <value>1</value>
      </not-equals>
      <not-equals>
        <property>/instrumentation/radio-select/switch</property>
        <value>2</value>
      </not-equals>
      <not-equals>
        <property>/instrumentation/tacan/frequencies/selected-channel[4]</property>
        <value type="string">Y</value>
      </not-equals>
    </condition>
</animation>

<animation>
    <type>select</type>
    <object-name>tacanY</object-name>
    <condition>
      <not-equals>
        <property>/instrumentation/radio-select/switch</property>
        <value>0</value>
      </not-equals>
      <not-equals>
        <property>/instrumentation/radio-select/switch</property>
        <value>1</value>
      </not-equals>
      <not-equals>
        <property>/instrumentation/radio-select/switch</property>
        <value>2</value>
      </not-equals>
      <not-equals>
        <property>/instrumentation/tacan/frequencies/selected-channel[4]</property>
        <value type="string">X</value>
      </not-equals>
    </condition>
</animation>

<animation>
    <type>select</type>
    <object-name>decDot</object-name>
    <condition>
      <not-equals>
        <property>/instrumentation/radio-select/switch</property>
        <value>0</value>
      </not-equals>
      <not-equals>
        <property>/instrumentation/radio-select/switch</property>
        <value>3</value>
      </not-equals>
    </condition>
</animation>

