            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>crsnum10</object-name>
        <property>/instrumentation/nav[0]/radials/selected-deg</property>
        <factor>0.01</factor>
        <step>10</step>
		    <bias>0.005</bias>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>crsnum1</object-name>
        <property>/instrumentation/nav[0]/radials/selected-deg</property>
        <factor>0.1</factor>
        <step>1</step>
		    <bias>0.005</bias>
