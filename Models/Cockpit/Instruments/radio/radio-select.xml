<!-- ##################################  GREAT FREQUENCY KNOB ################################### -->

    <animation>
        <type>pick</type>
        <object-name>freqKnob</object-name>
        <visible>true</visible>

<!-- LEFT MOUSE AND SCROLL WHEEL UP-->
        <action>
            <button>0</button>
            <button>3</button>
            <repeatable>true</repeatable>
            <interval-sec>0.5</interval-sec>

        <!-- if ADF is selected -->
            <binding>
