              <property>/instrumentation/radio-select/switch</property>
              <step>-1</step>
              <min>0</min>
              <max>4</max>
              <wrap>1</wrap>
            </binding>
            <binding>
              <command>property-toggle</command>
              <property>/sim/sound/switch2</property>
            </binding>
        </action>

        <action>
            <button>3</button>
            <binding>
