        </center>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>crsPlane</object-name>
        <visible>false</visible>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <condition>
                    <not>
                        <property>devices/status/keyboard/shift</property>
                    </not>
                </condition>
                <command>property-adjust</command>
                <property>/instrumentation/nav[0]/radials/selected-deg</property>
                <step>10</step>
                <min>0</min>
                <max>360</max>
                <wrap>1</wrap>
            </binding>
            <binding>
                <condition>
                    <property>devices/status/keyboard/shift</property>
                </condition>
                <command>property-adjust</command>
                <property>/instrumentation/nav[0]/radials/selected-deg</property>
                <step>20</step>
                <min>0</min>
                <max>360</max>
                <wrap>1</wrap>
            </binding>
            <binding>
              <command>property-toggle</command>
              <property>/sim/sound/switch2</property>
            </binding>
        </action>

        <action>
            <button>1</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <condition>
                    <not>
                        <property>devices/status/keyboard/shift</property>
                    </not>
                </condition>
                <command>property-adjust</command>
                <property>/instrumentation/nav[0]/radials/selected-deg</property>
                <step>-10</step>
                <min>0</min>
                <max>360</max>
                <wrap>1</wrap>
            </binding>
            <binding>
                <condition>
                    <property>devices/status/keyboard/shift</property>
                </condition>
                <command>property-adjust</command>
                <property>/instrumentation/nav[0]/radials/selected-deg</property>
                <step>-20</step>
                <min>0</min>
                <max>360</max>
                <wrap>1</wrap>
            </binding>
            <binding>
              <command>property-toggle</command>
              <property>/sim/sound/switch2</property>
            </binding>
        </action>

        <action>
            <button>3</button>
            <binding>
                <condition>
                    <not>
                        <property>devices/status/keyboard/shift</property>
                    </not>
                </condition>
                <command>property-adjust</command>
                <property>/instrumentation/nav[0]/radials/selected-deg</property>
                <step>1</step>
                <min>0</min>
                <max>360</max>
                <wrap>1</wrap>
            </binding>
            <binding>
                <condition>
                    <property>devices/status/keyboard/shift</property>
                </condition>
                <command>property-adjust</command>
                <property>/instrumentation/nav[0]/radials/selected-deg</property>
                <step>5</step>
                <min>0</min>
                <max>360</max>
                <wrap>1</wrap>
            </binding>
            <binding>
              <command>property-toggle</command>
              <property>/sim/sound/switch2</property>
            </binding>
        </action>

        <action>
            <button>4</button>
            <binding>
                <condition>
                    <not>
                        <property>devices/status/keyboard/shift</property>
                    </not>
                </condition>
                <command>property-adjust</command>
                <property>/instrumentation/nav[0]/radials/selected-deg</property>
                <step>-1</step>
                <min>0</min>
                <max>360</max>
                <wrap>1</wrap>
            </binding>
            <binding>
                <condition>
                    <property>devices/status/keyboard/shift</property>
                </condition>
                <command>property-adjust</command>
                <property>/instrumentation/nav[0]/radials/selected-deg</property>
                <step>-5</step>
                <min>0</min>
                <max>360</max>
                <wrap>1</wrap>
            </binding>
            <binding>
              <command>property-toggle</command>
              <property>/sim/sound/switch2</property>
            </binding>
        </action>
    </animation>


