<?xml version="1.0"?>
<!-- 
Work together with the nav-dev.xml instrument
-->

<PropertyList>

<path>radio-select.ac</path>

<animation>
    <type>material</type>    
    <object-name>decDot</object-name>
    <object-name>crsnum1</object-name>
    <object-name>crsnum10</object-name>
    <object-name>crsnum100</object-name>
    <object-name>comm01</object-name>
    <object-name>comm001</object-name>
    <object-name>comm1</object-name>
    <object-name>comm10</object-name>
    <object-name>comm100</object-name>
    <object-name>nav01</object-name>
    <object-name>nav001</object-name>
    <object-name>nav1</object-name>
    <object-name>nav10</object-name>
    <object-name>nav100</object-name>
    <object-name>adf1</object-name>
    <object-name>adf10</object-name>
    <object-name>adf100</object-name>
    <object-name>adf1000</object-name>
    <object-name>tacan1</object-name>
    <object-name>tacan10</object-name>
    <object-name>tacan100</object-name>
    <object-name>tacanX</object-name>
    <object-name>tacanY</object-name>
      <condition>
        <greater-than>
          <property>sim/multiplay/generic/float[11]</property>
          <value>0</value>
        </greater-than>
      </condition>
      <emission>
          <red>2.5</red>
          <green>1.2</green>
          <blue>0.6</blue>
        <factor-prop>sim/multiplay/generic/float[11]</factor-prop>
      </emission>
</animation>


<animation>
    <type>material</type>    
    <object-name>decDot</object-name>
    <object-name>crsnum1</object-name>
    <object-name>crsnum10</object-name>
    <object-name>crsnum100</object-name>
    <object-name>comm01</object-name>
    <object-name>comm001</object-name>
    <object-name>comm1</object-name>
    <object-name>comm10</object-name>
    <object-name>comm100</object-name>
    <object-name>nav01</object-name>
    <object-name>nav001</object-name>
    <object-name>nav1</object-name>
    <object-name>nav10</object-name>
    <object-name>nav100</object-name>
    <object-name>adf1</object-name>
    <object-name>adf10</object-name>
    <object-name>adf100</object-name>
    <object-name>adf1000</object-name>
    <object-name>tacan1</object-name>
    <object-name>tacan10</object-name>
    <object-name>tacan100</object-name>
    <object-name>tacanX</object-name>
    <object-name>tacanY</object-name>
      <condition>
        <less-than>
          <property>sim/multiplay/generic/float[11]</property>
          <value>1</value>
        </less-than>
      </condition>
    <emission>
      <red>0.028</red>
      <green>0.014</green>
      <blue>0.007</blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
</animation>


<animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>crsKnob</object-name>
    <object-name>freqKnob</object-name>
    <object-name>freqKnobDec</object-name>
    <object-name>selKnob</object-name>
      <condition>
        <greater-than>
          <property>sim/multiplay/generic/float[11]</property>
          <value>0</value>
        </greater-than>
      <not><property>/sim/rendering/rembrandt/enabled</property></not>
      </condition>
      <emission>      
        <red>1.25</red>
        <green>0.6</green>
        <blue>0.3</blue>
        <factor-prop>sim/multiplay/generic/float[11]</factor-prop>
      </emission>
</animation>

<animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>crsKnob</object-name>
    <object-name>freqKnob</object-name>
    <object-name>freqKnobDec</object-name>
    <object-name>selKnob</object-name>
      <condition>
        <less-than>
          <property>sim/multiplay/generic/float[11]</property>
          <value>1</value>
        </less-than>
      <not><property>/sim/rendering/rembrandt/enabled</property></not>
      </condition>
      <emission>
        <red>0.01</red>
        <green>0.01</green>
        <blue>0.01</blue>
        <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      </emission>
</animation>

<animation>
    <type>select</type>
    <object-name>adf1</object-name>
    <object-name>adf10</object-name>
    <object-name>adf100</object-name>
    <object-name>adf1000</object-name>
    <condition>
      <equals>
        <property>/instrumentation/radio-select/switch</property>
        <value>0</value>
      </equals>
    </condition>
</animation>

<animation>
    <type>select</type>
    <object-name>comm01</object-name>
    <object-name>comm001</object-name>
    <object-name>comm1</object-name>
    <object-name>comm10</object-name>
    <object-name>comm100</object-name>
    <condition>
      <equals>
        <property>/instrumentation/radio-select/switch</property>
        <value>1</value>
      </equals>
    </condition>
</animation>

<animation>
    <type>select</type>
    <object-name>nav01</object-name>
    <object-name>nav001</object-name>
    <object-name>nav1</object-name>
    <object-name>nav10</object-name>
    <object-name>nav100</object-name>
    <condition>
      <equals>
        <property>/instrumentation/radio-select/switch</property>
        <value>2</value>
      </equals>
    </condition>
</animation>

<animation>
    <type>select</type>
    <object-name>tacan1</object-name>
    <object-name>tacan10</object-name>
    <object-name>tacan100</object-name>
    <object-name>tacanX</object-name>
    <object-name>tacanY</object-name>
