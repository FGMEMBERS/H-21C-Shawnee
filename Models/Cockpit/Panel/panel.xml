<PropertyList>

	<path>panel.ac</path>

  <animation>
    <type>material</type>
    <object-name>Panel</object-name>
    <condition>
      <equals>
        <property>sim/model/livery/file</property>
        <value>Marine</value>
      </equals>
    </condition>
    <texture>panel.png</texture>
  </animation>


  <animation>
    <type>material</type>
    <object-name>Panel</object-name>
    <condition>
      <equals>
        <property>sim/model/livery/file</property>
        <value>13DBLE</value>
      </equals>
    </condition>
    <texture>milpanel.png</texture>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Panel</object-name>
    <condition>
      <not><property>/sim/rendering/rembrandt/enabled</property></not>
    </condition>    
    <emission>
      <red>1.25</red>
      <green>0.6</green>
      <blue>0.3</blue>
      <factor-prop>sim/multiplay/generic/float[11]</factor-prop>
    </emission>
  </animation>

<!-- Magnetic-Compass -->
  <model>
    <name>Magnetic Compass</name>
    <path>Aircraft/H-21C-Shawnee/Models/Cockpit/Instruments/mag-compass/mag-compass.xml</path>
      <offsets>
      <x-m>-0.130</x-m>
      <y-m>-0.017</y-m>
      <z-m>0.340</z-m>
      <pitch-deg>0</pitch-deg>
      </offsets>
  </model>

