      <offset-m>3.2</offset-m>
      <axis>
          <x>2.972</x>
          <y>0.0</y>
          <z>1.243</z>
      </axis>
  </animation>

  <animation>
      <type>billboard</type>
      <object-name>redlight.2</object-name>
  </animation>

  <animation>
      <type>dist-scale</type>
